@model Technician
@{
    ViewData["Title"] = "Get Technician";
}

<h2>Technician</h2>

<form asp-action="List" method="post" asp-antiforgery="true">
    <input type="hidden" asp-for="Name" />
    <div class="form-group">
        <select class="form-control" asp-for="TechnicianId" asp-items="ViewBag.Technicians">
            <option value="" selected>Select a Technician</option>
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Select" class="btn btn-primary" />
    </div>
</form>